{
  "default_provider": "anthropic",
  "providers": {
    "openai": {
      "type": "openai",
      "api_key": "",
      "model": "gpt-4"
    },
    "mistral": {
      "type": "mistral",
      "api_key": "",
      "model": "mistral-medium"
    },
    "anthropic": {
      "type": "anthropic",
      "api_key": "",
      "model": "claude-3-7-sonnet-latest",
      "version": "2023-06-01"
    },
    "google": {
      "type": "google",
      "api_key": "",
      "model": "gemini-pro"
    },
    "llama": {
      "type": "llama",
      "api_key": "",
      "model": "llama-3-70b",
      "endpoint": "http://localhost:8080"
    }
  },
  "cache": {
    "enabled": true,
    "type": "memory",
    "ttl": 3600,
    "max_entries": 1000
  },
  "rag": {
    "enabled": false,
    "vector_store": "memory",
    "embeddings": "openai",
    "chunk_size": 1000,
    "chunk_overlap": 200
  },
  "tracing": {
    "enabled": false,
    "type": "console",
    "sample_rate": 1.0
  },
  "custom_provider_paths": [
    "./custom_providers"
  ]
}
